game.Players.LocalPlayer.Chatted:connect(function(msg)
	if msg == "wprint" or msg == "wPrint" then
	table.foreach(workspace:children(),print)
    end
end)



game.Players.LocalPlayer.Chatted:connect(function(msg)
	if msg == "cmds" or msg == "Cmds" then
	print 'Music, Darude, Bastile, Illuminati,Blox'
    end
end)



game.Players.LocalPlayer.Chatted:connect(function(msg)
	if msg == "Tphatgd" or msg == "tophat" then
	game:service'InsertService':LoadAsset(1563352):children()[1].Parent=workspace.gavinsharp100
game.workspace.gavinsharp100.PinkEye:Destroy()

    end
end)
