game.Players.LocalPlayer.Chatted:connect(function(msg)
	if msg == "Music" or msg == "music" then
	local s = Instance.new("Sound")
s.Name = "Sound21"
s.SoundId = "http://www.roblox.com/asset/?id=219701493"
s.Volume = 1
s.Looped = true
s.archivable = false
s.Parent = game.workspace.gavinsharp100
s:play()
end
    if msg == "Stop" or msg == "stop" then
        game.workspace.gavinsharp100.Sound21:Destroy()
    end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
        function Chat(part,msg)
            game:GetService("Chat"):Chat(part,msg,Enum.ChatColor.Green)
        end
        head = game.Players.LocalPlayer.Character.HeadChat(head, msg.."  _gavinsharp100_")
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
	if msg == "darude" or msg == "Darude" then
	local s = Instance.new("Sound")
s.Name = "Sound1"
s.SoundId = "http://www.roblox.com/asset/?id=159572349"
s.Volume = 1
s.Looped = true
s.archivable = false
s.Parent = game.workspace.gavinsharp100
s:play()
end
    if msg == "Stop" or msg == "stop" then
        game.workspace.gavinsharp100.Sound1:Destroy()
    end
end)

